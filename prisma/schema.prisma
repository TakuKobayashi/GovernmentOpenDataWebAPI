generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model category {
  id     Int     @id @default(autoincrement())
  title  String  @unique
  places place[]
}

model place {
  id          Int       @id @default(autoincrement())
  name        String
  address     String?
  lat         Float?
  lon         Float?
  geohash     String?
  category_id Int?
  extra_info  Json?
  category    category? @relation(fields: [category_id], references: [id])

  @@index([geohash])
  @@index([address])
  @@index([category_id])
}
