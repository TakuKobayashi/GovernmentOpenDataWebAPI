  color: var(--metrotokyo-color-button-disabled);
}

.language_more[data-button-secondary-state='disabled'] > .language_more_text,
.language_more:disabled > .language_more_text {
  cursor: not-allowed;
  color: var(--metrotokyo-color-button-disabled);
}

