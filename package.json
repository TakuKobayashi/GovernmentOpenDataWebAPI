    "commander": "^12.0.0",
    "documentation": "^14.0.3",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.6",
    "typescript": "^5.4.5",
